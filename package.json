{
  "name": "@michzuerch/privaterautomarkt8",
  "description": "Homepage Privater Automarkt using Astro + Tailwind CSS.",
  "version": "0.9.8",
  "private": true,
  "homepage": "https://github.com/michzuerch/PrivaterAutomarkt8#readme",
  "author": {
    "name": "Michael Zuercher",
    "email": "michzuerch@gmail.com",
    "url": "https://github.io/michzuerch"
  },
  "repository": "github:michzuerch/PrivaterAutomarkt8",
  "keywords": [
    "Homepage",
    "Privater Automarkt",
    "billig-car.de",
    "Radolfzell"
  ],
  "displayName": "Privater Automarkt 8",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "clean": "rimraf dist .astro node_modules",
    "build:clean": "sudo rm -rf ./playwright-report/mobile-* && sudo rm -rf ./playwright-report/desktop-*",
    "build:docker": "docker build -t privaterautomarkt8-playwright:latest .",
    "test:docker": "docker run --name privaterautomarkt8-playwright --rm -v $(pwd)/playwright-report:/app/playwright-report -p 9323:9323 --network host privaterautomarkt8-playwright:latest",
    "test": "LOCAL_TEST=1 npx playwright test",
    "test:snapshots": "LOCAL_TEST=1 npx playwright test --update-snapshots",
    "test:lighthouse": "npx lighthouse http://localhost:3000/ --output html --output-path lighthouse-result.html view",
    "preview": "astro preview",
    "astro": "astro",
    "format": "prettier -w .",
    "lint:eslint": "eslint . --ext .js,.ts,.astro",
    "subfont": "subfont -ir --no-fallbacks --silent --root dist"
  },
  "devDependencies": {
    "@astrojs/image": "0.16.1",
    "@astrojs/mdx": "^0.18.1",
    "@astrojs/partytown": "^1.1.1",
    "@astrojs/rss": "^2.3.0",
    "@astrojs/sitemap": "^1.2.1",
    "@astrojs/tailwind": "3.1.1",
    "@astrolib/analytics": "^0.3.0",
    "@astrolib/seo": "^0.3.0",
    "@fontsource/inter": "^4.5.15",
    "@playwright/test": "^1.31.2",
    "@tailwindcss/typography": "^0.5.9",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "astro": "2.1.3",
    "astro-compress": "1.1.35",
    "astro-icon": "^0.8.0",
    "daisyui": "^2.51.4",
    "leaflet": "1.9.3",
    "eslint": "^8.36.0",
    "eslint-plugin-astro": "^0.26.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "lighthouse": "^10.0.2",
    "limax": "^2.1.0",
    "mdast-util-to-string": "^3.1.1",
    "playwright": "^1.31.2",
    "prettier": "^2.8.6",
    "prettier-plugin-astro": "^0.8.0",
    "prettier-plugin-tailwindcss": "^0.2.5",
    "rimraf": "^4.4.0",
    "tailwindcss-fluid-type": "^2.0.3",
    "reading-time": "^1.5.0",
    "sharp": "^0.31.3",
    "subfont": "^6.12.4",
    "svgo": "2.8.0",
    "tailwindcss": "^3.2.7",
    "flowbite": "^1.6.4",
    "typescript": "^5.0.2"
  },
  "engines": {
    "node": ">=16.12.0"
  }
}
